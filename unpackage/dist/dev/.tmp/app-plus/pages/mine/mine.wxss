
.header {
	height: 400rpx;
	width: 100%;
	background: -webkit-gradient(linear, left top, left bottom, from(#CEA15A), to(#F3CB84));
	background: -webkit-linear-gradient(#CEA15A, #F3CB84);
	background: -o-linear-gradient(#CEA15A, #F3CB84);
	background: linear-gradient(#CEA15A, #F3CB84);
}
.header .section1 {
	background-image: url(../../static/img/mine.6d70c908.png);
	background-repeat: no-repeat;
	background-size: 100% 100%;
	-webkit-border-radius: 8rpx;
	        border-radius: 8rpx;
	overflow: hidden;
	background-color: transparent;
	height: 340rpx;
	margin-top: 40rpx;
	padding: 20rpx;
	width: 710rpx;
	-webkit-box-sizing:border-box;
	        box-sizing:border-box;
}
.section1 image {
	width: 100rpx;
	height: 100rpx;
	-webkit-border-radius: 50rpx;
	        border-radius: 50rpx;
	margin: 10rpx 0 0 0;
}
.section1 .name {
	color: #ffffff;
	font-size: 40rpx;
	padding-left: 1em;
}
.section1 .info {
	color: #ffffff;
	font-size: 24rpx;
}
.section1 .level {
	color: #ffffff;
	font-size: 32rpx;
	text-align: center;
	margin-top: 60rpx;
}
.section1 .level .line {
	background: #ffffff;
	display: inline-block;
	width: 1px;
	height: 50rpx;
}
.section1 .level .num {
	line-height: 3em;
}
.list .item {
	color: #333333;
	font-size: 32rpx;
	line-height: 2em;
	padding: 20rpx;
	border-bottom: 1px solid #E6E6E6;
}
.list .item .forward {
	float: right;
	line-height: 2em;
}

