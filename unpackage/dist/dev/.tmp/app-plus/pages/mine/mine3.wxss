
.list_wrap{margin:0 20rpx; width: 710rpx; margin-top: 20rpx; margin-bottom: 100rpx;}
.list .item{color: #333333; background: #FFFFFF; font-size: 32rpx; line-height: 2em; -webkit-border-radius: 8rpx; border-radius: 8rpx; padding: 20rpx; margin-bottom: 20rpx;}
.list .item .flex1{-webkit-align-self: flex-start;-ms-flex-item-align: start;align-self: flex-start; margin-top: 20rpx;}
.list .item image{width: 80rpx; height: 80rpx;}
.list .item .name{color: #333333; font-size: 32rpx;}
.list .item .type{color: #999999; font-size: 26rpx;}
.list .item .num{color: #333333; font-size: 28rpx;}
.bottomBtn{width: 710rpx; position: fixed; bottom: 10rpx;  background: #CCA366; color: #ffffff; -webkit-border-radius: 8rpx; border-radius: 8rpx;
