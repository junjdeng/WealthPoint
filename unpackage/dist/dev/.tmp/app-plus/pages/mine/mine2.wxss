
.section1 {
	width: 750rpx;
	padding: 40rpx 0;
	background: -webkit-linear-gradient(313deg, #CEA15A, #F3CB84);
	background: -o-linear-gradient(313deg, #CEA15A, #F3CB84);
	background: linear-gradient(137deg, #CEA15A, #F3CB84);
	text-align: center;
}
.section1 .title {
	font-size: 32rpx;
	color: #ffffff;
	line-height: 1.6em;
	margin-top: 10rpx;
}
.section1 .num {
	font-size: 64rpx;
	color: #ffffff;
	line-height: 1.6em;
	font-weight: bold;
	margin-bottom: 30rpx;
}
.section1 .want {
	color: #FFFFFF;
	font-size: 32rpx;
	border: 1px solid #FFFFFF;
	-webkit-border-radius: 16rpx;
	        border-radius: 16rpx;
	padding: 10rpx 30rpx;
	margin: 0 40rpx;
}
.section_title {
	color: #333333;
	font-size: 28rpx;
	font-weight: bold;
	line-height: 2em;
}
.section2 {
	margin: 20rpx;
	width: 710rpx;
}
.tree_wrap {
	font-size: 14px;
	color: #888888;
	margin-top: 20rpx;
	padding: 10rpx;
	background: #ffffff;
	line-height: 28rpx;
}
.tree_wrap .tree_first {position:relative;z-index:1}
.tree_wrap .tree_first_title {
}
.tree_wrap .tree_second {
	position:relative;
	z-index:10;
}
.tree_wrap .tree_second_title {
	text-indent: 1em;
}
.tree_wrap .tree_second_title::before{
	position:absolute;
	content:'';
	width:2rpx;
	height:-webkit-calc(100%);
	height:calc(100%);
	background: #999999;
	top:-10rpx;
	left:16rpx;
}
.tree_wrap .tree_second::before {
	position:absolute;
	content:'';
	width:44rpx;
	height:2rpx;
	background: #999999;
	top:24rpx;
	left:16rpx;
}
.tree_wrap .tree_second::after {
	position:absolute;
	content:'';
	width:2rpx;
	height:30rpx;
	background: #999999;
	top:24rpx;
	left:-60rpx;
	z-index:100;
}
.tree_wrap .tree_third {
	text-indent: 4em;
	position:relative;
}
.tree_wrap .tree_third::before {
		position:absolute;
		content:'';
		width:40rpx;
		height:-webkit-calc(86%);
		height:calc(86%);
		background: transparent;
		top:-30%;
		left:70rpx;
		z-index:1;
		border-left:2rpx solid #999999;
		border-bottom:2rpx solid #999999;
}
.tree_wrap .tree_txt {
	padding: 10rpx 0;
}

