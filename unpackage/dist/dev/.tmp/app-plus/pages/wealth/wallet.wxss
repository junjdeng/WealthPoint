
.section1 {
	width: 750rpx;
	padding: 80rpx 0;
	background: -webkit-linear-gradient(313deg, #CEA15A, #F3CB84);
	background: -o-linear-gradient(313deg, #CEA15A, #F3CB84);
	background: linear-gradient(137deg, #CEA15A, #F3CB84);
	text-align: center;
}
.section1 .title {
	font-size: 32rpx;
	color: #ffffff;
	line-height: 1.6em;
	margin-top: 10rpx;
}
.section1 .num {
	font-size: 64rpx;
	color: #ffffff;
	line-height: 1.6em;
	font-weight: bold;
	margin-bottom: 30rpx;
}
.want {
	color: #FFFFFF;
	width:710rpx;
	font-size: 32rpx;
	text-align: center;
	-webkit-border-radius: 12rpx;
	        border-radius: 12rpx;
	background: -webkit-linear-gradient(313deg, #CEA15A, #F3CB84);
	background: -o-linear-gradient(313deg, #CEA15A, #F3CB84);
	background: linear-gradient(137deg, #CEA15A, #F3CB84);
	padding: 20rpx 40rpx;
	margin-top:80rpx;
	-webkit-box-sizing:border-box;
	        box-sizing:border-box;
}
.section_title {
	color: #333333;
	font-size: 28rpx;
	font-weight: bold;
	padding:20rpx 40rpx;
	-webkit-box-sizing:border-box;
	        box-sizing:border-box;
