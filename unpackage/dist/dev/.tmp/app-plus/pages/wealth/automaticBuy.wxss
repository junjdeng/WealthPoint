
.form_wrap {
	margin-top: 20rpx;
}
.form_wrap .dj_form {
	display: inline-block;
	width: 750rpx;
	position: relative;
}
.dj_form .uni-form-item {
	margin-bottom: 2rpx;
	padding: 20rpx;
	background: #ffffff;
}
/* .dj_form .uni-form-item image{width:100upx; height: 100upx; border-radius: 50upx;} */
.dj_form .uni-form-item .title {
	color: #333333;
	font-size: 28rpx;
	line-height: 2em;
}
.dj_form .uni-input {
	color: #333333;
	font-size: 28rpx;
	text-align: right;
}
.dj_form .uni-btn-v {
	position: absolute;
	bottom: -200rpx;
	left: 20rpx;
	width: 710rpx;
}
.dj_form .uni-btn-v button {
	background: #cca366;
	font-size: 32rpx;
	color: #ffffff;
}
.dj_form .uni-form-item .forward {
	text-align: right;
}
.getImg {
	width: 160rpx;
	height: 160rpx;
	border: 1px solid #f7f7f7;
	position: relative;
	margin-left: 40rpx;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
}
.getImg input {
	display: block;
	width: 100%;
	height: 100%;
	opacity: 1;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 100;
}
.showImg {
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 99;
}
.showImg image {
	width: 100%;
	height: 100%;
	-o-object-fit: cover;
	   object-fit: cover;
}

