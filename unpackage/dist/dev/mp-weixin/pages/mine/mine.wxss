
.header{height: 500rpx; width: 100%; background: -webkit-gradient(linear,left top, left bottom,from(#CEA15A),to(#F3CB84)); background: -o-linear-gradient(#CEA15A,#F3CB84); background: linear-gradient(#CEA15A,#F3CB84);}
.header .section1{background:-o-linear-gradient(22deg,rgba(204,204,204,0) 0%,rgba(255,255,255,1) 100%);background:linear-gradient(68deg,rgba(204,204,204,0) 0%,rgba(255,255,255,1) 100%);
height: 340rpx; margin-top: 96rpx;-webkit-box-shadow:2px 2px 4px 0px rgba(0,0,0,0.2);box-shadow:2px 2px 4px 0px rgba(0,0,0,0.2); padding: 20rpx; width: 670rpx;}
.section1 image{width: 100rpx; height: 100rpx; border-radius: 50rpx;}
.section1 .name{color: #ffffff; font-size: 40rpx; padding-left: 1em;}
.section1 .info{color: #ffffff; font-size: 24rpx;
}
.section1 .level{color: #ffffff; font-size: 32rpx; text-align: center; margin-top: 60rpx;}
.section1 .level .line{background:#ffffff; display: inline-block; width: 1px; height: 50rpx;}
.section1 .level .num{line-height: 3em;}
.list .item{color: #333333; font-size: 32rpx; line-height: 2em; padding: 20rpx; border-bottom: 1px solid #E6E6E6;}
.list .item .forward{float: right; line-height: 2em;}


