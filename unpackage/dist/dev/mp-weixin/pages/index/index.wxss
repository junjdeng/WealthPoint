
.header {
	height: 128rpx;
	background: #ffffff;
	text-align: center;
	width: 100%;
}
.header image {
	height: 128rpx;
	width: 100%;
}
.section1 {
	padding: 20rpx 0;
}
.section_row {
	width: 100%;
}
.item {
	text-align: center;
	padding: 20rpx;
}
.item image {
	width: 64rpx;
	height: 64rpx;
}
.item view {
	font-size: 28rpx;
	color: #333333;
	line-height: 1.6em;
}
.section2 {
	background: #f5f5f5;
}
.notice .sound_icon {
	position: relative;
	padding-right: 20rpx;
}
.notice text {
	font-size: 24rpx;
	color: #333333;
	display: inline-block;
	width: 570rpx;
	height: 50rpx;
	line-height: 50rpx;
}
.notice ._span {
	font-size: 24rpx;
	color: #888888;
	display: inline-block;
	height: 50rpx;
	line-height: 50rpx;
}
.section_title {
	color: #333333;
	font-size: 32rpx;
	font-weight: bold;
}
.section3 {
	padding: 20rpx;
	width: 670rpx;
	margin-top: 0;
}
.section3 .total {
	font-size: 56rpx;
	color: #FF5533;
	text-align: center;
}
.section3 .total_sub {
	font-size: 28rpx;
	color: #FF5533;
	text-align: center;
}
.section3 .condition_card {
	color: #999999;
	font-size: 28rpx;
	text-align: center;
	padding-top: 40rpx;
}
.section4 {
	padding: 20rpx;
	margin-bottom: 20rpx;
	width: 670rpx;
}
.canvasView {
	width: 100%;
	height: 400rpx;
}

