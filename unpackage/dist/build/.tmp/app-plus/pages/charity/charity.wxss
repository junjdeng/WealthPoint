
.section1{padding: 20rpx; width: 670rpx; text-align: center;}
.section1 .title{font-size: 28rpx; color: #333333; line-height: 1.6em; font-weight: bold;}
.section1 .num{font-size: 48rpx; line-height: 2em; font-weight: bold;}
.section1 .total_card{background:rgba(204,163,102,0.1); border-radius: 16rpx; margin-top: 20rpx; padding: 10rpx 5rpx;}
.section1 .total_card .num{font-size: 32rpx; color: #333333; font-weight: bold;}
.section1 .total_card .title{font-size: 28rpx; color: #666666;}
.section_title{font-size: 36rpx; color: #333333; line-height: 2em; height: 2em; text-align: left; width: 710rpx; padding: 20rpx 30rpx;margin-bottom:30rpx;}
.section_title ._span{float: right;font-size: 28rpx; color: #999999;}
.list{width: 710rpx;}
.list .item{ background: #ffffff; border-radius: 16rpx; padding: 20rpx; margin-bottom: 20rpx;}
.list .item image{width: 160rpx; height: 160rpx;border-radius: 16rpx;}
.list .item .flex2{height: 160rpx;}
.list .item .content{-webkit-align-self: flex-start;-ms-flex-item-align: start;align-self: flex-start;}
.list .item .title{font-size: 32rpx; color: #333333; line-height: 2.2em;}
.list .item .sub_title{font-size: 24rpx; color: #999999; line-height: 1.8em;}
view{
	-webkit-box-sizing:border-box!important;
	        box-sizing:border-box!important;
